{
  "extends": ["react-app"],
  "rules": {
    "no-unused-vars": 1,
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-one-expression-per-line": 0,
    "implicit-arrow-linebreak": 0,
    "import/no-unresolved": 0,
    "consistent-return": 0
  },
  "env": {
    "browser": true,
    "node": true
  }
}
